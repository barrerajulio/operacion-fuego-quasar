{
  "individually": true,
  "npmClient": "yarn",
  "packages": ["projects/*", "libraries/*"],
  "version": "0.0.2",
  "useWorkspaces": true,
  "command": {
    "publish": {
      "registry": "https://npm.pkg.github.com"
    },
    "version": {
      "allowBranch": ["master"],
      "message": "chore(release): publish %s"
    }
  }
}
